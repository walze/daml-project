{
  "name": "daml-project",
  "version": "1.0.0",
  "private": true,
  "type": "module",
  "scripts": {
    "build": "concurrently --timings --kill-others 'pnpm:build:*'",
    "build:client": "vite build",
    "build:server": "tsc -P server/tsconfig.node.json",
    "clear": "rimraf node_modules pnpm-lock.yaml",
    "dev": "concurrently --kill-others 'pnpm:dev:*'",
    "dev:client": "vite",
    "logs": "docker compose logs -f",
    "prepare": "husky install",
    "preview": "vite preview",
    "start:server": "pm2-runtime tsc/server/index.js",
    "test:lint": "eslint --ext .ts,.tsx,.js,.jsx server src"
  },
  "config": {
    "commitizen": {
      "path": "cz-conventional-changelog"
    }
  },
  "dependencies": {
    "@headlessui/react": "^1.7.3",
    "@heroicons/react": "^2.0.12",
    "@sveltejs/vite-plugin-svelte": "^1.1.0",
    "@tailwindcss/forms": "^0.5.3",
    "@tsconfig/node18-strictest": "^1.0.0",
    "@tsconfig/svelte": "^3.0.0",
    "@types/assert": "^1.5.6",
    "@types/body-parser": "^1.19.2",
    "@types/cors": "^2.8.12",
    "@types/express": "^4.17.14",
    "@types/glob": "^8.0.0",
    "@types/js-cookie": "^3.0.2",
    "@types/jsonwebtoken": "^8.5.9",
    "@types/lodash": "^4.14.186",
    "@types/node": "^18.11.7",
    "@typescript-eslint/eslint-plugin": "^5.41.0",
    "@typescript-eslint/parser": "^5.41.0",
    "assert": "npm:browser-assert@latest",
    "body-parser": "^1.20.1",
    "concurrently": "^7.5.0",
    "cors": "^2.8.5",
    "cz-conventional-changelog": "^3.3.0",
    "dotenv": "^16.0.3",
    "esbuild": "^0.15.12",
    "esbuild-plugin-alias": "^0.2.1",
    "eslint": "^8.26.0",
    "eslint-config-prettier": "^8.5.0",
    "eslint-plugin-prettier": "^4.2.1",
    "express": "^4.18.2",
    "glob": "^8.0.3",
    "https-proxy-agent": "^5.0.1",
    "husky": "^8.0.1",
    "js-cookie": "^3.0.1",
    "jsonwebtoken": "^8.5.1",
    "lodash": "^4.17.21",
    "node-fetch": "^3.2.10",
    "nodemon": "^2.0.20",
    "pm2": "^5.2.2",
    "postcss": "^8.4.18",
    "postcss-plugin": "^1.0.0",
    "prettier": "^2.7.1",
    "rimraf": "^3.0.2",
    "rxjs": "^7.5.7",
    "sass": "^1.55.0",
    "slugify": "^1.6.5",
    "svelte": "^3.52.0",
    "svelte-check": "^2.9.2",
    "svelte-preprocess": "^4.10.7",
    "tailwindcss": "^3.2.1",
    "ts-node": "^10.9.1",
    "ts-node-dev": "^2.0.0",
    "tslib": "^2.4.0",
    "typescript": "^4.8.4",
    "vite": "^3.2.1",
    "zustand": "^4.1.3"
  },
  "packageManager": "pnpm@7.14.1"
}
